<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 映射器要求：命名空间为映射器（接口）的完整名 -->
<mapper namespace="com.mini.dao.UserDao">


	<select id="selectAllUsers" resultType="com.mini.entity.User" >
		SELECT
		NAME  "name",
		AGE "age"
		FROM TBL_MINI_USER
	</select>


	<insert id="2" parameterType="string">

	</insert>	

	<update id="1" parameterType="map">

	</update>
	
</mapper>